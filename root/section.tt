[% # USE Dumper %]
[% # Dumper.dump_html( title, s_path, sname, url, secs ) %]

[% INCLUDE header.tt
   title = 'TheTyee.ca - Mobile'
   body_id = 'index'
%]

<h2>
[% title %]
</h2>

[% IF results %] 
        <ul>
            [% FOREACH item IN results %]
                    <li><a href="[% item.path %]">[% item.title %]</a>
                    </li>
            [% END %]
        </ul>
[% END %]
[% INCLUDE footer.tt
%]
